{
  "name": "transcript_reason_town_fm",
  "version": "0.1.0",
  "scripts": {
    "start": "yarn build && node src/Transfer.bs.js",
    "build": "bsb -make-world",
    "build:watch": "bsb -make-world -w",
    "build:rei":
      "bsc -bs-re-out lib/bs/src/Feed-Transcriptreasontownfm.cmi >> src/Feed.rei",
    "job":
      "yarn clean:audio && docker build -t reasonfm . && docker run -v `pwd`/audio:/app/audio -t reasonfm",
    "clean:audio": "rm -f audio/*.flac && rm -f audio/*.mp3",
    "clean": "bsb -clean-world"
  },
  "keywords": ["BuckleScript"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^2.2.3",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "@google-cloud/speech": "^1.3.0",
    "@google-cloud/storage": "^1.6.0",
    "bs-fetch": "^0.2.1",
    "bs-pixl-xml": "^0.1.0",
    "isomorphic-fetch": "^2.2.1",
    "mp3-split": "^4.0.0",
    "pixl-xml": "^1.0.13"
  }
}
